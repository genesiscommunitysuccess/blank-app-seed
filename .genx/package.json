{
  "name": "@genesislcap/blank-app-seed-config",
  "description": "Genesis Blank App Seed Configuration",
  "version": "5.7.0-prerelease.29",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.js\""
  },
  "genxSeedConfig": {
    "exclude": [
      ".git",
      "package.json",
      "package-lock.json",
      ".releaserc",
      "LICENSE",
      "CHANGELOG.md",
      ".github",
      ".genx/tests",
      ".genx/scripts",
      ".genx/.releaserc"
    ],
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".yml",
          ".sh",
          ".js",
          ".kt",
          ".kts",
          ".ts",
          ".css",
          ".md",
          ".properties",
          ".html",
          ".gitignore",
          ".xml",
          ".iml"
        ],
        "exclude": [
          "design-tokens.json"
        ]
      }
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.3.0",
    "prettier": "^3.2.5"
  }
}
