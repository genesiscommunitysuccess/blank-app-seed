{
  "name": "@genesislcap/blank-app-seed-config",
  "description": "Genesis Blank App Seed Configuration",
  "version": "2.5.0",
  "license": "Apache-2.0",
  "genxSeedConfig": {
    "exclude": [
      ".git",
      "package.json",
      "package-lock.json",
      ".releaserc",
      "LICENSE",
      "CHANGELOG.md",
      ".github",
      ".genx/tests",
      ".genx/scripts",
      ".genx/.releaserc"
    ],
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".yml",
          ".sh",
          ".json",
          ".js",
          ".kts",
          ".ts",
          ".md",
          ".properties",
          ".html",
          ".gitignore",
          ".xml",
          ".iml"
        ]
      }
    }
  }
}
