<ng-container *ngIf="canUserViewElement('{{config.permissions.updateRight}}'); else notPermitted{{ config.index }}">
  <foundation-form
    design-system-prefix="rapid"
    resourceName="{{config.resourceName}}"
    {{#if config.uischema}}
    [uischema]="tile{{ config.index }}.uischema"
    {{/if}}
  >
  </foundation-form>
</ng-container>

<ng-template #notPermitted{{ config.index }}>
  <not-permitted-component></not-permitted-component>
</ng-template>

