<ng-container *ngIf="canUserViewElement('{{config.permissions.viewRight}}'); else notPermitted{{ config.index }}">
  <rapid-g2plot-chart
    type="{{ config.type }}"
    [config]="tile{{ config.index }}.chartConfig"
  >
    <chart-datasource
      resourceName="{{ config.resourceName }}"
      server-fields="{{ config.xField }} {{ config.yField }}"
    ></chart-datasource>
  </rapid-g2plot-chart>
</ng-container>

<ng-template #notPermitted{{ config.index }}>
  <not-permitted-component></not-permitted-component>
</ng-template>


